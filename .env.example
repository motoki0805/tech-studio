# GitHub Configuration
# Your GitHub username
GITHUB_USERNAME=your_github_username

# GitHub Personal Access Token
# Generate one at: https://github.com/settings/tokens
# Required scopes: repo (for accessing repository data)
GITHUB_TOKEN=your_github_personal_access_token

# Comma-separated list of repository names to exclude from portfolio
# Example: repo1,repo2,repo3
GITHUB_IGNORE_REPOS=

# The name of this site's repository
GITHUB_CURRENT_REPO_NAME=your_repository_name

# Maintenance Mode
# Set to "true" to enable maintenance mode, "false" to disable
NEXT_PUBLIC_IS_MAINTENANCE=false
